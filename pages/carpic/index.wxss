/**index.wxss**/
.topTitle{
  height: 70rpx;
  line-height: 70rpx;
  font-size: 32rpx;
  color: #666;
}
.color_blue{
  color:rgba(0,151,255,1);
  padding-left: 10rpx;
}
.top_image{
  position: absolute;
  z-index: 1;
  top:0;
  left:0;
  right:0;
  width: 100%;
}

.myCanvas {
  position: absolute;
  top: -9999px;
  left: -9999px;
}
